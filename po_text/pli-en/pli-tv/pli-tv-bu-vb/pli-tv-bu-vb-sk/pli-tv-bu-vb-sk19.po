msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk19:0.2"
msgid "Mahāvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk19:0.3"
msgid "Sekhiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bu-vb-sk19:0.4"
msgid "Ujjagghikavagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bu-vb-sk19:0.5"
msgid "19. Sīsappacālakasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-bu-vb-sk19:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bu-vb-sk19:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhū sīsappacālakaṃ antaraghare gacchanti sīsaṃ olambentā …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk19:6.3"
msgid "pe …."
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc2
msgctxt "pli-tv-bu-vb-sk19:7.1"
msgid "“Na sīsappacālakaṃ antaraghare gamissāmīti sikkhā karaṇīyā”ti."
msgstr ""

#. HTML: </b></p><p>
#. REF: sc3
msgctxt "pli-tv-bu-vb-sk19:8.1"
msgid "Na sīsappacālakaṃ antaraghare gantabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk19:8.2"
msgid "Sīsaṃ paggahetvā gantabbaṃ."
msgstr ""

msgctxt "pli-tv-bu-vb-sk19:8.3"
msgid "Yo anādariyaṃ paṭicca sīsappacālakaṃ antaraghare gacchati sīsaṃ olambento, āpatti dukkaṭassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc4
msgctxt "pli-tv-bu-vb-sk19:9.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bu-vb-sk19:9.2"
msgid "asañcicca … pe …"
msgstr ""

msgctxt "pli-tv-bu-vb-sk19:9.3"
msgid "ādikammikassāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc5
msgctxt "pli-tv-bu-vb-sk19:10.1"
msgid "Navamasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

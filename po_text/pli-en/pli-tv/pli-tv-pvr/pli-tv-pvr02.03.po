#. HTML: </p><h1>
msgctxt "pli-tv-pvr2.3:0.4"
msgid "3. Vipattivāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli5.80
msgctxt "pli-tv-pvr2.3:5.1"
msgid "Avassutāya bhikkhuniyā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantiyā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr2.3:5.2"
msgid "Avassutāya bhikkhuniyā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantiyā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr2.3:5.3"
msgid "siyā sīlavipattiṃ, siyā ācāravipattiṃ …"
msgstr ""

msgctxt "pli-tv-pvr2.3:5.4"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.3:5.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr2.3:6.1"
msgid "Dadhiṃ viññāpetvā bhuñjantiyā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr2.3:6.2"
msgid "Dadhiṃ viññāpetvā bhuñjantiyā āpattiyo catunnaṃ vipattīnaṃ ekaṃ vipattiṃ bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr2.3:6.3"
msgid "ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr2.3:6.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc3
msgctxt "pli-tv-pvr2.3:7.1"
msgid "Vipattivāro niṭṭhito tatiyo."
msgstr ""

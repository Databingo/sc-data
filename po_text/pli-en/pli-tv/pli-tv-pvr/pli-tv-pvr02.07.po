msgctxt "pli-tv-pvr2.7:0.4"
msgid "7. Samathavāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1
msgctxt "pli-tv-pvr2.7:5.1"
msgid "Avassutāya bhikkhuniyā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantiyā āpattiyo sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr2.7:5.2"
msgid "Avassutāya bhikkhuniyā avassutassa purisapuggalassa kāyasaṃsaggaṃ sādiyantiyā āpattiyo sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr2.7:5.3"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca …"
msgstr ""

msgctxt "pli-tv-pvr2.7:5.4"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr2.7:5.5"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2, pts-vp-pli5.81
msgctxt "pli-tv-pvr2.7:6.1"
msgid "Dadhiṃ viññāpetvā bhuñjantiyā āpattiyo sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr2.7:6.2"
msgid "Dadhiṃ viññāpetvā bhuñjantiyā āpattiyo sattannaṃ samathānaṃ tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr2.7:6.3"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca."
msgstr ""

msgctxt "pli-tv-pvr2.7:6.4"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc3
msgctxt "pli-tv-pvr2.7:7.1"
msgid "Samathavāro niṭṭhito sattamo."
msgstr ""

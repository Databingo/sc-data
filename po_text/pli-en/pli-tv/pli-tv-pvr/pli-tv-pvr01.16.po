#. HTML: <section class="sutta" id="pli-tv-pvr1.16"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-pvr1.16:0.1"
msgid "Parivāra"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.16:0.2"
msgid "Bhikkhuvibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-pvr1.16:0.3"
msgid "Dutiyabhāga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-pvr1.16:0.4"
msgid "8. Samuccayavāra"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli5.53
msgctxt "pli-tv-pvr1.16:5.1"
msgid "Methunaṃ dhammaṃ paṭisevanapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.2"
msgid "Methunaṃ dhammaṃ paṭisevanapaccayā catasso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.16:5.3"
msgid "Akkhāyite sarīre methunaṃ dhammaṃ paṭisevati, āpatti pārājikassa;"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.4"
msgid "yebhuyyena khāyite sarīre methunaṃ dhammaṃ paṭisevati, āpatti thullaccayassa;"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.5"
msgid "vaṭṭakate mukhe acchupantaṃ aṅgajātaṃ paveseti, āpatti dukkaṭassa;"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.6"
msgid "jatumaṭṭhake pācittiyaṃ—"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.7"
msgid "methunaṃ dhammaṃ paṭisevanapaccayā imā catasso āpattiyo āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.16:5.8"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ kati vipattiyo bhajanti?"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.9"
msgid "Sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.10"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhanti?"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.11"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.12"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammanti?"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.13"
msgid "Tā āpattiyo catunnaṃ vipattīnaṃ dve vipattiyo bhajanti—"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.14"
msgid "siyā sīlavipattiṃ siyā ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.16:5.15"
msgid "Sattannaṃ āpattikkhandhānaṃ catūhi āpattikkhandhehi saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.16"
msgid "siyā pārājikāpattikkhandhena, siyā thullaccayāpattikkhandhena, siyā pācittiyāpattikkhandhena, siyā dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr1.16:5.17"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhanti—"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.18"
msgid "kāyato ca cittato ca samuṭṭhanti, na vācato."
msgstr ""

msgctxt "pli-tv-pvr1.16:5.19"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.16:5.20"
msgid "Sattannaṃ samathānaṃ, tīhi samathehi sammanti—"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.21"
msgid "siyā sammukhāvinayena ca, paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena ca …"
msgstr ""

msgctxt "pli-tv-pvr1.16:5.22"
msgid "pe …."
msgstr ""

msgctxt "pli-tv-pvr1.16:5.23"
msgid "(1)"
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-pvr1.16:6.1"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karaṇapaccayā kati āpattiyo āpajjati?"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.2"
msgid "Anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karaṇapaccayā ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.16:6.3"
msgid "Dukkaṭaṃ—"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.4"
msgid "anādariyaṃ paṭicca udake uccāraṃ vā passāvaṃ vā kheḷaṃ vā karaṇapaccayā imaṃ ekaṃ āpattiṃ āpajjati."
msgstr ""

msgctxt "pli-tv-pvr1.16:6.5"
msgid "Sā āpatti catunnaṃ vipattīnaṃ kati vipattiyo bhajati?"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.6"
msgid "Sattannaṃ āpattikkhandhānaṃ katihi āpattikkhandhehi saṅgahitā?"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.7"
msgid "Channaṃ āpattisamuṭṭhānānaṃ katihi samuṭṭhānehi samuṭṭhāti?"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.8"
msgid "Catunnaṃ adhikaraṇānaṃ katamaṃ adhikaraṇaṃ?"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.9"
msgid "Sattannaṃ samathānaṃ katihi samathehi sammati?"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.10"
msgid "Sā āpatti catunnaṃ vipattīnaṃ ekaṃ vipattiṃ bhajati—"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.11"
msgid "ācāravipattiṃ."
msgstr ""

msgctxt "pli-tv-pvr1.16:6.12"
msgid "Sattannaṃ āpattikkhandhānaṃ ekena āpattikkhandhena saṅgahitā—"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.13"
msgid "dukkaṭāpattikkhandhena."
msgstr ""

msgctxt "pli-tv-pvr1.16:6.14"
msgid "Channaṃ āpattisamuṭṭhānānaṃ ekena samuṭṭhānena samuṭṭhāti—"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.15"
msgid "kāyato ca cittato ca samuṭṭhāti, na vācato."
msgstr ""

msgctxt "pli-tv-pvr1.16:6.16"
msgid "Catunnaṃ adhikaraṇānaṃ, āpattādhikaraṇaṃ."
msgstr ""

msgctxt "pli-tv-pvr1.16:6.17"
msgid "Sattannaṃ samathānaṃ tīhi samathehi sammati—"
msgstr ""

msgctxt "pli-tv-pvr1.16:6.18"
msgid "siyā sammukhāvinayena ca paṭiññātakaraṇena ca, siyā sammukhāvinayena ca tiṇavatthārakena cāti."
msgstr ""

msgctxt "pli-tv-pvr1.16:6.19"
msgid "(2)"
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc3
msgctxt "pli-tv-pvr1.16:7.1"
msgid "Samuccayavāro niṭṭhito aṭṭhamo."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.16:7.2"
msgid "Aṭṭhapaccayavārā niṭṭhitā."
msgstr ""

#. HTML: <br>
msgctxt "pli-tv-pvr1.16:7.3"
msgid "Mahāvibhaṅge soḷasamahāvārā niṭṭhitā."
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc4
#. VAR: Bhikkhuvibhaṅgamahāvāro niṭṭhito → niṭṭhito ca mahāvibhaṅgo (bj)
msgctxt "pli-tv-pvr1.16:8.1"
msgid "Bhikkhuvibhaṅgamahāvāro niṭṭhito."
msgstr ""

#. HTML: <section class="sutta" id="pli-tv-bi-vb-as1-7"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-as1-7:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-as1-7:0.2"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-as1-7:0.3"
msgid "Adhikaraṇasamatha"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-as1-7:0.4"
msgid "Adhikaraṇasamathā:"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-as1-7:0.5"
msgid "Tiṇavatthāraka"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.351
#. VAR: panāyyāyo → panayyāyo (bj, s1-3, pts1)
msgctxt "pli-tv-bi-vb-as1-7:5.1"
msgid "Ime kho panāyyāyo satta adhikaraṇasamathā dhammā uddesaṃ āgacchanti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
#. VAR: paṭiññāya kāretabbaṃ → paṭiññātakaraṇaṃ (s1-3)
msgctxt "pli-tv-bi-vb-as1-7:6.1"
msgid "Uppannuppannānaṃ adhikaraṇānaṃ samathāya vūpasamāya sammukhāvinayo dātabbo, sativinayo dātabbo, amūḷhavinayo dātabbo, paṭiññāya kāretabbaṃ, yebhuyyasikā, tassapāpiyasikā, tiṇavatthārakoti."
msgstr ""

#. HTML: </p><p>
#. REF: sc3
msgctxt "pli-tv-bi-vb-as1-7:7.1"
msgid "Uddiṭṭhā kho, ayyāyo, satta adhikaraṇasamathā dhammā."
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:7.2"
msgid "Tatthāyyāyo pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:7.3"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:7.4"
msgid "Dutiyampi pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:7.5"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:7.6"
msgid "Tatiyampi pucchāmi—"
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:7.7"
msgid "“kaccittha parisuddhā”?"
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:7.8"
msgid "Parisuddhetthāyyāyo, tasmā tuṇhī, evametaṃ dhārayāmīti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc4
msgctxt "pli-tv-bi-vb-as1-7:8.1"
msgid "Adhikaraṇasamathā niṭṭhitā."
msgstr ""

#. HTML: </p><p>
#. REF: sc5
msgctxt "pli-tv-bi-vb-as1-7:9.1"
msgid "Uddiṭṭhaṃ kho, ayyāyo, nidānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:9.2"
msgid "Uddiṭṭhā aṭṭha pārājikā dhammā."
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:9.3"
msgid "Uddiṭṭhā sattarasa saṃghādisesā dhammā."
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:9.4"
msgid "Uddiṭṭhā tiṃsa nissaggiyā pācittiyā dhammā."
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:9.5"
msgid "Uddiṭṭhā chasaṭṭhisatā pācittiyā dhammā."
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:9.6"
msgid "Uddiṭṭhā aṭṭha pāṭidesanīyā dhammā."
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:9.7"
msgid "Uddiṭṭhā sekhiyā dhammā."
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:9.8"
msgid "Uddiṭṭhā satta adhikaraṇasamathā dhammā."
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:9.9"
msgid "Ettakaṃ tassa bhagavato suttāgataṃ suttapariyāpannaṃ anvaddhamāsaṃ uddesaṃ āgacchati."
msgstr ""

msgctxt "pli-tv-bi-vb-as1-7:9.10"
msgid "Tattha sabbāheva samaggāhi sammodamānāhi avivadamānāhi sikkhitabbanti."
msgstr ""

#. HTML: </p><p class="endsection">
#. REF: sc6
msgctxt "pli-tv-bi-vb-as1-7:10.1"
msgid "Bhikkhunivibhaṅgo niṭṭhito."
msgstr ""

#. HTML: </p><p class="endbook">
#. REF: sc7
msgctxt "pli-tv-bi-vb-as1-7:11.1"
msgid "Pācittiyapāḷi niṭṭhitā."
msgstr ""

#. HTML: <section class="sutta" id="pli-tv-bi-vb-pc43"><article><div class="hgroup"><p class="division">
msgctxt "pli-tv-bi-vb-pc43:0.1"
msgid "Theravāda Vinaya"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc43:0.2"
msgid "Bhikkhunivibhaṅga"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc43:0.3"
msgid "Pācittiyakaṇḍa"
msgstr ""

#. HTML: </p><p>
msgctxt "pli-tv-bi-vb-pc43:0.4"
msgid "Cittāgāravagga"
msgstr ""

#. HTML: </p><h1>
msgctxt "pli-tv-bi-vb-pc43:0.5"
msgid "43. Suttakantanasikkhāpada"
msgstr ""

#. HTML: </h1></div><p>
#. REF: sc1, pts-vp-pli4.300
msgctxt "pli-tv-bi-vb-pc43:6.1"
msgid "Tena samayena buddho bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:6.2"
msgid "Tena kho pana samayena chabbaggiyā bhikkhuniyo suttaṃ kantanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:6.3"
msgid "Manussā vihāracārikaṃ āhiṇḍantā passitvā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:6.4"
msgid "“kathañhi nāma bhikkhuniyo suttaṃ kantissanti, seyyathāpi gihiniyo kāmabhoginiyo”ti."
msgstr ""

#. HTML: </p><p>
#. REF: sc2
msgctxt "pli-tv-bi-vb-pc43:7.1"
msgid "Assosuṃ kho bhikkhuniyo tesaṃ manussānaṃ ujjhāyantānaṃ khiyyantānaṃ vipācentānaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:7.2"
msgid "Yā tā bhikkhuniyo appicchā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:7.3"
msgid "tā ujjhāyanti khiyyanti vipācenti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:7.4"
msgid "“kathañhi nāma chabbaggiyā bhikkhuniyo suttaṃ kantissantī”ti … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:7.5"
msgid "“saccaṃ kira, bhikkhave, chabbaggiyā bhikkhuniyo suttaṃ kantantī”ti?"
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:7.6"
msgid "“Saccaṃ, bhagavā”ti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:7.7"
msgid "Vigarahi buddho bhagavā … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:7.8"
msgid "kathañhi nāma, bhikkhave, chabbaggiyā bhikkhuniyo suttaṃ kantissanti."
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:7.9"
msgid "Netaṃ, bhikkhave, appasannānaṃ vā pasādāya … pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:7.10"
msgid "evañca pana, bhikkhave, bhikkhuniyo imaṃ sikkhāpadaṃ uddisantu—"
msgstr ""

#. HTML: </p><p><b class="rule">
#. REF: sc3
msgctxt "pli-tv-bi-vb-pc43:8.1"
msgid "“Yā pana bhikkhunī suttaṃ kanteyya, pācittiyan”ti."
msgstr ""

#. HTML: </b></p><p><b>
#. REF: sc4
msgctxt "pli-tv-bi-vb-pc43:9.1"
msgid "Yā panāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc43:9.2"
msgid "yā yādisā … pe …"
msgstr ""

#. HTML: <b>
msgctxt "pli-tv-bi-vb-pc43:9.3"
msgid "bhikkhunīti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc43:9.4"
msgid "… pe …"
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:9.5"
msgid "ayaṃ imasmiṃ atthe adhippetā bhikkhunīti."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc5
msgctxt "pli-tv-bi-vb-pc43:10.1"
msgid "Suttaṃ nāma"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc43:10.2"
msgid "cha suttāni—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:10.3"
msgid "khomaṃ, kappāsikaṃ, koseyyaṃ, kambalaṃ, sāṇaṃ, bhaṅgaṃ."
msgstr ""

#. HTML: </p><p><b>
#. REF: sc6
msgctxt "pli-tv-bi-vb-pc43:11.1"
msgid "Kanteyyāti"
msgstr ""

#. HTML: </b>
msgctxt "pli-tv-bi-vb-pc43:11.2"
msgid "sayaṃ kantati, payoge dukkaṭaṃ."
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:11.3"
msgid "Ujjavujjave āpatti pācittiyassa."
msgstr ""

#. HTML: </p><p>
#. REF: sc7
msgctxt "pli-tv-bi-vb-pc43:12.1"
msgid "Anāpatti—"
msgstr ""

msgctxt "pli-tv-bi-vb-pc43:12.2"
msgid "kantitasuttaṃ kantati, ummattikāya, ādikammikāyāti."
msgstr ""

#. HTML: </p><p class="endsutta">
#. REF: sc8
msgctxt "pli-tv-bi-vb-pc43:13.1"
msgid "Tatiyasikkhāpadaṃ niṭṭhitaṃ."
msgstr ""

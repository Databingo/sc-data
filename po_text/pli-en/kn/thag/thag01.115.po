msgctxt "thag1.115:1.1"
msgid "Theragāthā"
msgstr "Verses of the Senior Monks"

#. HTML: </p><p>
msgctxt "thag1.115:2.1"
msgid "Ekakanipāta"
msgstr "The Book of the Ones"

#. HTML: </p><p>
msgctxt "thag1.115:3.1"
msgid "Dvādasamavagga"
msgstr "Chapter Twelve"

#. HTML: </p><h1>
msgctxt "thag1.115:4.1"
msgid "1.115. Mahānāmattheragāthā"
msgstr "1.115. Mahānāma"

#. HTML: </h1></div><blockquote class="gatha"><p><a class="sc" id="1"></a><a class="vns" id="vns115"></a><a class="vnp" id="vnp115"></a>
msgctxt "thag1.115:5.1"
msgid "“Esāvahiyyase pabbatena,"
msgstr "By Mount Nesādaka,"

#. HTML: <br>
#. VAR: Bahukuṭajasallakikena → ... sallarikena (bj, s2, s3)
msgctxt "thag1.115:5.2"
msgid "Bahukuṭajasallakikena;"
msgstr "with its famous covering"

#. HTML: <br>
msgctxt "thag1.115:5.3"
msgid "Nesādakena girinā,"
msgstr "of abundant shrubs and trees,"

#. HTML: <br>
msgctxt "thag1.115:5.4"
msgid "Yasassinā paricchadenā”ti."
msgstr "you’re found deficient."

#. HTML: </p></blockquote><p class="endsutta">
msgctxt "thag1.115:6.1"
msgid "…"
msgstr ""

msgctxt "thag1.115:6.2"
msgid "Mahānāmo thero …."
msgstr ""


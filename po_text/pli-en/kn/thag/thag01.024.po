msgctxt "thag1.24:1.1"
msgid "Theragāthā"
msgstr "Verses of the Senior Monks"

#. HTML: </p><p>
msgctxt "thag1.24:2.1"
msgid "Ekakanipāta"
msgstr "The Book of the Ones"

#. HTML: </p><p>
msgctxt "thag1.24:3.1"
msgid "Tatiyavagga"
msgstr "Chapter Three"

#. HTML: </p><h1>
msgctxt "thag1.24:4.1"
msgid "1.24. Sugandhattheragāthā"
msgstr "1.24. Sugandha"

#. HTML: </h1></div><blockquote class="gatha"><p><a class="sc" id="1"></a><a
#. class="vns" id="vns24"></a><a class="vnp" id="vnp24"></a>
msgctxt "thag1.24:5.1"
msgid "“Anuvassiko pabbajito,"
msgstr "See the excellence of the teaching!"

#. HTML: <br>
msgctxt "thag1.24:5.2"
msgid "passa dhammasudhammataṃ;"
msgstr "Just one rainy season after I went forth,"

#. HTML: <br>
msgctxt "thag1.24:5.3"
msgid "Tisso vijjā anuppattā,"
msgstr "I attained the three knowledges"

#. HTML: <br>
msgctxt "thag1.24:5.4"
msgid "kataṃ buddhassa sāsanan”ti."
msgstr "and fulfilled the Buddha’s instructions."

#. HTML: </p></blockquote><p class="endsutta">
msgctxt "thag1.24:6.1"
msgid "…"
msgstr ""

msgctxt "thag1.24:6.2"
msgid "Sugandho thero …."
msgstr ""


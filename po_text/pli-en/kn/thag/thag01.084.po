msgctxt "thag1.84:0.1"
msgid "Theragāthā"
msgstr "Verses of the Senior Monks"

#. HTML: </p><p>
msgctxt "thag1.84:0.2"
msgid "Ekakanipāta"
msgstr "The Book of the Ones"

#. HTML: </p><p>
msgctxt "thag1.84:0.3"
msgid "Navamavagga"
msgstr "Chapter Nine"

#. HTML: </p><h1>
msgctxt "thag1.84:0.4"
msgid "1.84. Nītattheragāthā"
msgstr "1.84. Nīta"

#. HTML: </h1></div><blockquote class="gatha"><p><a class="sc" id="1"></a><a class="vns" id="vns84"></a><a class="vnp" id="vnp84"></a><a class="pts" id="pts13"></a>
msgctxt "thag1.84:5.1"
msgid "“Sabbarattiṃ supitvāna,"
msgstr "Sleeping all night,"

#. HTML: <br>
msgctxt "thag1.84:5.2"
msgid "divā saṅgaṇike rato;"
msgstr "happily socializing by day,"

#. HTML: <br>
msgctxt "thag1.84:5.3"
msgid "Kudāssu nāma dummedho,"
msgstr "when will the fool"

#. HTML: <br>
msgctxt "thag1.84:5.4"
msgid "dukkhassantaṃ karissatī”ti."
msgstr "make an end of suffering?"

#. HTML: </p></blockquote><p class="endsutta">
msgctxt "thag1.84:6.1"
msgid "…"
msgstr ""

msgctxt "thag1.84:6.2"
msgid "Nīto thero …."
msgstr ""

